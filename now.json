{
  "version": 2,
  "name": "gitvis",
  "scope": "axelekwall",
  "public": true,
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "alias": ["gitvis.axelekwall.se"],
  "github": {
    "enabled": false
  },
  "build": {
    "env": {
      "VUE_APP_GRAPHQL_HTTP": "https://api.github.com/graphql"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ]
}
